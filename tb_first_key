TD_tb_first_key_only.test_first_key_registration ;
    %vpi_call/w 3 104 "$display", "Testing first key registration..." {0 0 0};
    %pushi/vec4 14, 0, 4;
    %store/vec4 v0x12c6583c0_0, 0, 4;
    %delay 200, 0;
    %load/vec4 v0x12c6586e0_0;
    %cmpi/e 1, 0, 4;
    %jmp/0xz  T_0.0, 4;
    %vpi_call/w 3 111 "$display", "PASS: First key registered and held correctly" {0 0 0};
    %vpi_call/w 3 112 "$display", "  Held key code: %h", v0x12c6586e0_0 {0 0 0};
    %jmp T_0.1;
T_0.0 ;
    %vpi_call/w 3 114 "$display", "FAIL: First key not registered (held_key_code=%h)", v0x12c6586e0_0 {0 0 0};
T_0.1 ;
    %delay 100, 0;
    %load/vec4 v0x12c6586e0_0;
    %cmpi/e 1, 0, 4;
    %jmp/0xz  T_0.2, 4;
    %vpi_call/w 3 121 "$display", "PASS: First key maintained in held state" {0 0 0};
    %jmp T_0.3;
T_0.2 ;
    %vpi_call/w 3 123 "$display", "FAIL: First key not maintained (held_key_code = %h)", v0x12c6586e0_0 {0 0 0};
T_0.3 ;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x12c6583c0_0, 0, 4;
    %delay 50, 0;
    %load/vec4 v0x12c6586e0_0;
    %cmpi/e 0, 0, 4;
    %jmp/0xz  T_0.4, 4;
    %vpi_call/w 3 131 "$display", "PASS: Key released properly" {0 0 0};
    %jmp T_0.5;
T_0.4 ;
    %vpi_call/w 3 133 "$display", "FAIL: Key not released (held_key_code = %h)", v0x12c6586e0_0 {0 0 0};
T_0.5 ;
    %end;
