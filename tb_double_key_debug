E_0x154608550 .event/or E_0x154608550/0, E_0x154608550/1;
    .scope S_0x154607850;
T_0 ;
    %wait E_0x154608550;
    %load/vec4 v0x154621880_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x154621670_0, 0;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x1546217d0_0, 0;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x154621720_0, 0;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x154621990_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x154621990_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 3;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
