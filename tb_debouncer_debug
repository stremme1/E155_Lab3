    %callf/str TD_tb_debouncer_debug.get_state_name, S_0x105115c00;
    %vpi_call/w 4 93 "$display", "[%0t] CLK=%b RST=%b KEY_CODE=%s COL=%b KEY_DET=%b KEY_VAL=%b DEB_KEY=%s SCAN_STOP=%b STATE=%s COUNT=%0d", $time, v0x105115f10_0, v0x105116420_0, S<2,str>, v0x105115fd0_0, v0x1051162c0_0, v0x105116370_0, S<1,str>, v0x1051164d0_0, S<0,str>, v0x1051151f0_0 {0 0 3};
    %end;
